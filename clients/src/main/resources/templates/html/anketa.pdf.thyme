<html xmlns:th="http://www.thymeleaf.org">
<head>
    <style>
        * {
            font-family: Arial Unicode MS;
        }

        table {
            font-size: 9pt;
            width:100%;
            border: 1px solid black;
        }
         table td {
          border: 1px solid black;
        }

        .check {
            /*background-color: #5A9CC5;*/
        }

        h1, h2 {text-align: center;}

        .hystory {
            font-size: 7pt;
        }

        .right {
            float:right;
        }

        .left {
            float:left;
        }
        .contract {
            padding-left: 20px;
        }
    </style>

</head>
<body>
<h1>Заявка № [[${app.getId()}]]</h1>
<h2 th:text="${app.getClient().getName()}"></h2>
<table style="height: 656px;">
<tbody>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Название организации</td>
<td style="padding-top: 10px; height: 18px; width: 602px;" th:text="${app.getClient().getName()}" colspan="3"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">ИНН/ОГРН</td>
<td style="padding-top: 10px; height: 18px; width: 602px;" colspan="3">[[${app.getClient().getInn()}]] / [[${app.getClient().getOgrn()}]]</td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 482px;" colspan="2"><b>Информация о клиенте</b></td>
<td style="padding-top: 10px; height: 18px; width: 383px;" colspan="2"><b>KYC</b></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Тариф</td>
<td style="padding-top: 10px; height: 18px; width: 213px;">[[${billingPlan}]]</td>
<td style="padding-top: 10px; height: 18px; width: 227px;">Скоринг-бал</td>
<td style="padding-top: 10px; height: 18px; width: 150px;" th:text="${konturCheck == null} ? 'не производилась' : ${konturCheck}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Email</td>
<td style="padding-top: 10px; height: 18px; width: 213px;">[[${app.getClient().getEmail()}]]</td>
<td style="padding-top: 10px; height: 18px; width: 227px;">Проверка 550п организации</td>
<td style="padding-top: 10px; height: 18px; width: 150px;" th:text="${P550Check == null} ? 'не производилась' : ${P550Check}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Телефон</td>
<td style="padding-top: 10px; height: 18px; width: 213px;">[[${app.getClient().getPhone()}]]</td>
<td style="padding-top: 10px; height: 18px; width: 227px;">Проверка 550п руководителя</td>
<td style="padding-top: 10px; height: 18px; width: 150px;" th:text="${P550CheckHead == null} ? 'не производилась' : ${P550CheckHead}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Руководитель</td>
<td style="padding-top: 10px; height: 18px; width: 213px;"> [[${head}]] </td>
<td style="padding-top: 10px; height: 18px; width: 227px;">Проверка 550п учредителей</td>
<td style="padding-top: 10px; height: 18px; width: 150px;" th:text="${P550CheckFounder == null} ? 'не производилась' : ${P550CheckFounder}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Город</td>
<td style="padding-top: 10px; height: 18px; width: 213px;" th:text="${app.getCity().getName()}"></td>
<td style="padding-top: 10px; height: 18px; width: 227px;">Проверка паспорта</td>
<td style="padding-top: 10px; height: 18px; width: 150px;" th:text="${passportCheck == null} ? 'не производилась' : ${passportCheck}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Статус</td>
<td style="padding-top: 10px; height: 18px; width: 213px;">[[${status}]]</td>
<td style="padding-top: 10px; height: 18px; width: 227px;">Аресты</td>
<td style="padding-top: 10px; height: 18px; width: 150px;" th:text="${arrests == null} ? 'не производилась' : ${arrests}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Номер счета</td>
<td style="padding-top: 10px; height: 18px; width: 213px;">[[${app.getAccount().getAccountNumber()}]]</td>
<td style="padding-top: 10px; height: 18px; width: 227px;"></td>
<td style="padding-top: 10px; height: 18px; width: 150px;"></td>
</tr>
<tr style="height: 19px;">
<td colspan="4" style="height: 19px; width: 871px;"><b>Паспортные данные</b></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Серия</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${passportSer == null} or ${passportSer == ''} ? '---------' : ${passportSer}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Номер</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${passportNumber == null} or ${passportNumber == ''} ? '--------' : ${passportNumber}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Дата выдачи</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${passportDate == null} or ${passportDate == ''} ? '-------' : ${passportDate}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Код подразделения</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${passportCode == null} or ${passportCode == ''} ? '---------' : ${passportCode}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Кем выдан</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${passportIssuer == null} or ${passportIssuer == ''} ? '---------' : ${passportIssuer}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Дата рождения</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${clientDateOfBirth == null} or ${clientDateOfBirth == ''} ? '---------' : ${clientDateOfBirth}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Место рождения</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${clientPlaceOfBirth == null} or ${clientPlaceOfBirth == ''} ? '---------' : ${clientPlaceOfBirth}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">СНИЛС</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${snils}"></td>
</tr>
<tr style="height: 19px;">
<td colspan="4" style="height: 19px; width: 871px;"><b>Опросник</b></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Налогообложение</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${tax}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Выручка</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${income == null} ? 'не указана' : ${income}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Наличие бухгалтера</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${isChiefAccountant}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Обязанности по ведению б/у переданы</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${accountantNoSignPermission == null} ? 'не указано' : ${accountantNoSignPermission}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Фактическая численность компании</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${realCompanySize == null} ? 'не указана' : ${realCompanySize}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Официальный сайт</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${offSite == null} ? 'не указан' : ${offSite}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Чем занимается компания</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${businessType == null} ? 'не указано' : ${businessType}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Адрес регистрации</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;">[[${app.getClient().getResidentAddress()}]]</td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Адрес фактического проживания</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${realLocationAddress == null} ? 'не указано'
 : ${realLocationAddress}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">ИНН контрагентов плательщиков</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${supplierContragentsInn == null} ?
 'не указана' : ${supplierContragentsInn}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">ИНН контрагентов получателей</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${contragentsInn == null} ?
 'не указана' : ${contragentsInn}"></td>
</tr>
<tr style="height: 18px;">
<td style="padding-top: 10px; height: 18px; width: 263px;">Виды договоров</td>
<td colspan="3" style="padding-top: 10px; height: 18px; width: 602px;" th:text="${contractTypes == null} ?
 'не указано' : ${contractTypes}"></td>
</tr>
</tbody>
</table>

</body>
</html>
